.modal-dialog {
  width: 500px;
  z-index: 9999;
}

.container {
  display: flex;
  height: 100vh;
  width: 100%;
  flex-direction: column;
  background-color: #f2f2f2;
  overflow: hidden;

  // 导航栏
  .head {
    flex: 0 0 100px;
    display: flex;
    justify-content: space-between;
    margin-bottom: 5px;
    background: #fff;

    .logo-box {
      width: 720px;
      padding: 8px 18px 0 74px;
      height: 100px;

      .logo-img {
        width: 85px;
        height: 85px;
        border-radius: 50%;
        background: url(../img/logo.png) 0 0 no-repeat;
        background-size: 100% 100%;
        display: inline-flex;
      }

      .text {
        font-size: 33px;
        color: #93969b;
        display: inline-flex;
        vertical-align: top;
        line-height: 85px;
        padding-left: 29px;
      }
    }

    .return-box {
      width: 402px;
      display: flex;
      align-items: center;

      .return-main {
        width: 402px;
        height: 32px;
        display: flex;
        margin-top: 20px;
        padding-right: 15px;
      }

      .return-li {
        line-height: 32px;
        border-right: 1px solid #dcdcdc;
        font-size: 20px;
        color: #93969b;
        padding: 0 15px;
        text-align: center;

        span {
          cursor: pointer;
        }
      }

      .return-li:last-child {
        border: 0;
      }

      .return-li span:hover {
        color: #fa78b4;
      }

      .return-li.active {
        color: #fa78b4;
      }
    }
  }


  // 主要内容
  .main-body {
    flex: auto;

    .main-body-ifarme {
      width: 100%;
      height: 100%;
      display: none;
    }

    .main-body-ifarme.active {
      display: block;
    }
  }

  // 确认交卷
  .mark {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, .5);
    display: none;
  }

  .assign-box {
    width: 500px;
    height: auto;
    background-color: #fff;
    border-radius: 5px;
    box-shadow: 0 0 6px #a0a0a0;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -300px;
    margin-top: -200px;
    position: relative;

    .assignTop {
      padding: 15px;
      border-bottom: 1px solid #e5e5e5;
    }

    .contain {
      padding: 15px;
      font-size: 16px;
      border-bottom: 1px solid #e5e5e5;
    }

    .close {
      position: absolute;
      right: 18px;
      top: 18px;
      z-index: 999;
      cursor: pointer;
      font-size: 10px;

    }

    .bottom {
      height: 65px;
      display: flex;
      justify-content: flex-end;
      padding: 15px;
      text-align: right;

      .sure {
        margin-left: 15px;
      }
    }


  }

  // 模态框
  .modal-dialog {
    z-index: 9999;
    font-size: 0;

    .pic-left {
      display: inline-block;
      vertical-align: top;
      width: 45%;
      height: 389px;
      font-size: 16px;

      .pic {
        width: 200px;
        height: 200px;
        border: 1px solid #eee;
        margin: 30px auto;
        overflow: hidden;

        #imghead {
          filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod=image);
        }
      }

      .uplode {
        display: block;
        width: 200px;
        margin: 0 auto;
      }
    }

    .form-parbox {
      width: 55%;
      height: 100%;
      display: inline-block;
      vertical-align: top;
      font-size: 16px;

    }

    .modal-title {
      font-size: 20px;
    }

    .form-box {
      display: flex;
      flex-direction: column;

      .form-group {
        margin-bottom: 5px;
      }

      .layui-input-block {
        margin-left: 0;
      }

      .form-group:last-child {
        margin-bottom: 0;
      }

      .control-label {
        padding-right: 0;
        line-height: 34px;
      }

      .col-sm-10 {
        padding-left: 0;
      }

      .input-group {
        width: 100%;
      }

      .form-contain {
        padding: 6px;
      }

      .form-group:first-child .form-contain {
        padding-top: 3px;
      }
    }

    .layui-form-label {
      padding: 9px 0px
    }

    .layui-form-item {
      margin-bottom: 0;
    }

    .layui-input-block input[type=file] {
      padding-top: 6px;
    }

    .btns .layui-input-block {
      margin: 0 auto;
      margin-top: 20px;
      text-align: center;

      .layui-btn {
        width: 92px;
      }
    }
  }
}

// loading
.loading {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background-color: rgba(0, 0, 0, .3);
  display: flex;
  justify-content: center;
  align-items: center;

  .loading-img {
    width: 32px;
    height: 32px;
    background: url(../img/1.gif) 0 0 no-repeat;
    background-size: 100% 100%;
  }
}

// 查看试卷
.check-box {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: none;

  .check-mark {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .5);
  }

  .check-contain {
    width: 90%;
    height: 90%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    display: flex;
    flex-direction: column;
    background-color: #fff;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border: 1px solid rgba(0, 0, 0, .2);
    border-radius: 6px;
    outline: 0;
    -webkit-box-shadow: 0 3px 9px rgba(0, 0, 0, .5);
    box-shadow: 0 3px 9px rgba(0, 0, 0, .5);

    .check-head,
    .check-footer {
      flex: 0 0 70px;
    }

    .check-head {
      line-height: 70px;
      font-size: 23px;
      color: #6F737A;
      padding-left: 30px;
      border-bottom: 1px solid #e5e5e5;
    }

    .check-footer {
      color: #6F737A;
      text-align: center;
      font-size: 20px;
      position: relative;
      border-top: 1px solid #e5e5e5;

      .return-check {
        border: 1px solid #6F737A;
        width: 130px;
        position: absolute;
        top: 50%;
        left: 50%;
        margin-top: -15px;
        line-height: 30px;
        margin-left: -65px;
        cursor: pointer;
        letter-spacing: 2px;
        border-radius: 4px;
      }

      .return-check:hover {
        background-color: #6F737A;
        color: #fff;
      }
    }


    // 手持机考卷
    .check-body {
      flex: auto;
      // border-top: 1px solid #6F737A;
      // border-bottom: 1px solid #6F737A;

      .iframe-box {
        border: none;
        width: 100%;
        height: 100%;
      }
    }
  }

  // 模态框
  .modal-dialog {
    z-index: 9999;
    font-size: 0;

    .pic-left {
      display: inline-block;
