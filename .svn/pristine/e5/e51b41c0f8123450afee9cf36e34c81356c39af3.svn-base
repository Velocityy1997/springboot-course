.littleTip{
  width: 220px;
  height: 40px;
  box-shadow: 0 0 10px #909090;
  border-top-left-radius: 5px;
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -50px;
  font-size: 16px;
  color: #333;
  background: #fff;
  line-height: 40px;
  text-align: center;
  border-bottom-left-radius: 5px;
  display: none;

  .glyphicon{
    font-size: 12px;
    display: inline-block;
    width: 20px;
    height: 20px;
    position: absolute;
    right: 0px;
    top: 0px;
    line-height: 20px;
    cursor: pointer;
  }
}

// 考试状态
.test-state-box {
  width: 220px;
  box-shadow: 0 0 10px #909090;
  border-top-left-radius: 5px;
  position: absolute;
  right: 0;
  top: 50%;
  font-size: 16px;
  color: #333;
  background: #fff;
  text-align: center;
  border-bottom-left-radius: 5px;
  padding: 10px;
  cursor: default;

  // 考试标题 
  .test-title {
    color: #2d2d2d;
    line-height: 30px;
    font-size: 16px;
    font-weight: 550;
  }

  // 考试时间
  .test-time {
    color: #fb8ebf;
    font-weight: 550;
    font-size: 18px;
  }

  // 查看提交情况
  .view-submission{
    color: #79aaff;
    line-height: 30px;
    cursor: pointer;
  }
}