// 重写模态框
.modal-dialog {
  width: 90%;
  height: 92%;

  .modal-content {
    height: 100%;
    display: flex;
    flex-direction: column;
  }

  .modal-body {
    flex: auto;
    padding: 0;

    .iframe-box {
      width: 100%;
      height: 100%;
      border: none;
    }
  }

  .modal-header .close {
    font-size: 30px;
  }

  .modal-header {
    padding: 15px 20px;
  }

  .modal-footer {
    text-align: center;
  }
}

#exampleModal-check {
  .modal-dialog {
    height: 654px;
  }

  .modal-footer {
    border-top: none;
  }
}

