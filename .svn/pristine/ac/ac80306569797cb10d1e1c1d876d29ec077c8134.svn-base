.machine-box {
  display: flex;
  height: 100vh;
  flex-direction: column;
}

.machine-box .machine-head {
  flex: 0 0 70px;
  display: flex;
  font-size: 18px;
  background-color: #fff;
  color: #6f737a;
}

.machine-box .machine-head .return-box {
  flex: 1.5;
  display: flex;
  justify-content: center;
  align-items: center;
  cursor: pointer;
}

.machine-box .machine-head .return-box .return-img {
  width: 38px;
  height: 39px;
  background: url(../img/test/return.png) 0 0 no-repeat;
  background-size: 100% 100%;
  margin-left: 20px;
}

.machine-box .machine-head .return-box .return-text {
  color: #6f737a;
  margin-left: 14px;
}

.machine-box .machine-head .is-now-test {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
  background-image: linear-gradient(to right, #046b3a, #b6e2ae);
}

.machine-box .machine-head .is-now-test .test-img {
  width: 36px;
  height: 41px;
  background: url(../img/test/nowTest.png) 0 0 no-repeat;
  background-size: 100% 100%;
}

.machine-box .machine-head .is-now-test .test-text {
  color: #fff;
  letter-spacing: 1px;
  font-weight: 300;
  margin-left: 20px;
}

.machine-box .machine-head .test-time {
  flex: 4;
  display: flex;
  justify-content: center;
  align-items: center;
}

.machine-box .machine-head .test-time .remain-time-text {
  padding-left: 15px;
}

.machine-box .machine-head .test-time .time {
  padding-left: 10px;
}

.machine-box .machine-head .test-time .remain-time {
  color: #f959a3;
}

.machine-box .machine-head .hand {
  flex: 2;
  text-align: center;
  line-height: 70px;
  color: #fff;
  letter-spacing: 2px;
  background-color: #827de6;
  cursor: pointer;
  font-family: "SimHei";
  font-size: 23px;
}

.machine-box .machine-head .hand .collectIcon {
  vertical-align: top;
  margin-top: 22px;
  margin-right: 10px;
  font-size: 27px;
}

.machine-box .machine-head .collectButton {
  display: none;
}

.machine-box .machine-body {
  flex: auto;
  margin-top: 5px;
  background-color: #fff;
  display: flex;
}

.machine-box .machine-body .machine-body-left {
  flex: 0 0 35%;
  display: flex;
  justify-content: center;
}

