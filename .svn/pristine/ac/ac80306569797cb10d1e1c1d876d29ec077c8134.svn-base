.machine-box .machine-body .machine-body-right {
  flex: auto;
  border-left: 5px solid #f2f2f2;
  display: flex;
  flex-direction: column;
  position: relative;
}

.machine-box .machine-body .machine-body-right .item-bank {
  height: auto;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul {
  width: 100%;
  height: 100%;
  padding: 50px;
  font-size: 18px;
  color: #6f737a;
  letter-spacing: 2px;
  display: flex;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item {
  line-height: 30px;
  display: none;
  width: 100%;
  height: 100%;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item .answer-line {
  border: none;
  border-bottom: 1px solid #93969b;
  outline: 0;
  text-align: center;
  line-height: 22px;
  font-size: 16px;
  color: #6f737a;
  width: auto;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item .img-box {
  width: 700px;
  height: 335px;
  margin: 0 auto;
  margin-top: 40px;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item .img-box .img {
  width: 100%;
  height: 100%;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item .select-box {
  width: 100%;
  height: auto;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item .select-box .selectLi {
  width: 100%;
  height: 40px;
  line-height: 40px;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item .select-box .selectLi label {
  font-weight: normal;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item .select-box .selectLi label .selectRadio {
  margin-right: 10px;
}

.machine-box .machine-body .machine-body-right .item-bank .questions-ul .questions-item:nth-child(1) {
  display: block;
}

.machine-box .machine-body .machine-body-right .answer-bank {
  width: 100%;
  height: auto;
  display: none;
}

.machine-box .machine-body .machine-body-right .answer-bank .answer-box {
  width: 100%;
  height: 100%;
  padding: 0 50px 50px 50px;
  font-size: 16px;
  color: #6f737a;
  letter-spacing: 2px;
}

.machine-box .machine-body .machine-body-right .answer-bank .answer-box .answer-li {
  line-height: 30px;
  display: none;
  width: 100%;
  height: 100%;
}

.machine-box .machine-body .machine-body-right .answer-bank .answer-box .answer-li:nth-child(1) {
  display: block;
}

.machine-box .machine-body .machine-body-right .item-btn-ul {
  flex: 0 0 50px;
  display: flex;
  margin-bottom: 25px;
  justify-content: center;
  position: absolute;
  bottom: 45px;
  left: 50%;
  transform: translateX(-50%);
}

.machine-box .machine-body .machine-body-right .item-btn-ul .item-btn {
  width: 228px;
  height: 45px;
  font-size: 18px;
  text-align: center;
  line-height: 45px;
  border: 1px solid #718c7e;
  border-radius: 5px;
  margin-right: 25px;
  letter-spacing: 2px;
  cursor: pointer;
  color: #04331a;
  transition: all .5s ease;
}

.machine-box .machine-body .machine-body-right .item-btn-ul .answer {
  display: none;
}

.machine-box .machine-body .machine-body-right .item-btn-ul .item-btn.active,
.machine-box .machine-body .machine-body-right .item-btn-ul .item-btn:hover {
  background-color: #f959a3;
  color: #fff;
  border: 1px solid #fba1cb;
}

.modal-dialog {
  width: 500px;
  z-index: 9999;
}

.stu-grad-show {
