*{
  user-select: none;
}
.machine-box {
  display: flex;
  height: 100vh;
  flex-direction: column;


  .machine-head {
    flex: 0 0 70px;
    display: flex;
    font-size: 18px;
    background-color: #fff;
    color: #6f737a;

    // 返回
    .return-box {
      flex: 1.5;
      display: flex;
      justify-content: center;
      align-items: center;
      cursor: pointer;

      .return-img {
        width: 38px;
        height: 39px;
        background: url(../img/test/return.png) 0 0 no-repeat;
        background-size: 100% 100%;
        margin-left: 20px;
      }

      .return-text {
        color: #6f737a;
        margin-left: 14px;
      }
    }

    // 考核内容
    .is-now-test {
      flex: 2;
      display: flex;
      justify-content: center;
      align-items: center;
      background-image: linear-gradient(to right, #046b3a, #b6e2ae);

      .test-img {
        width: 36px;
        height: 41px;
        background: url(../img/test/nowTest.png) 0 0 no-repeat;
        background-size: 100% 100%;
      }

      .test-text {
        color: #fff;
        letter-spacing: 1px;
        font-weight: 300;
        margin-left: 20px;
      }
    }

    // 考核时间
    .test-time {
      flex: 4;
      display: flex;
      justify-content: center;
      align-items: center;

      .remain-time-text {
        padding-left: 15px;
      }

      .time {
        padding-left: 10px;
      }

      .remain-time {
        color: #f959a3;
      }
    }

    // 交卷
    .hand {
      flex: 2;
      text-align: center;
      line-height: 70px;
      color: #fff;
      letter-spacing: 2px;
      background-color: #827de6;
      cursor: pointer;
      font-family: "SimHei";
      font-size: 23px;

      .collectIcon {
        vertical-align: top;
        margin-top: 22px;
        margin-right: 10px;
        font-size: 27px;
      }
    }

    .collectButton {
      display: none;
    }
  }

