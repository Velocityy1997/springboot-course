._old path,
._old text {
  stroke: white;
}

.modal-dialog {
  width: 500px;
  z-index: 9999;
}

.moreSelect {
  -webkit-appearance: radio;
}

@font-face {
  font-family: 'myFont';
  /* 这个名字可以自己定义 */
  src: url('../fonts/Digital-7Mono.TTF');
}
@keyframes changeBackground{
  20%{
    background: #9AB79A;
  }
  80%{
    background:transparent;
  }
}