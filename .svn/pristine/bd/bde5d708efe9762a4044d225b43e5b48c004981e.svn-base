      .test-content {
        width: 25%;
        height: 100%;
        border-left: 1px solid #eee;
        display: inline-block;
        vertical-align: top;
        overflow-y: auto;
        .testParper{
          width: 100%;
          height: 75%;
          overflow-y: auto;
        }
        .questions-ul {
          width: 100%;
          height: auto;
          padding: 0 20px 20px 20px;
          font-size: 18px;
          color: #6f737a;
          letter-spacing: 2px;

          
          .questions-item {
            display: none;
            line-height: 30px;
            margin-top: 30px;

            .answer-line {
              border: none;
              border-bottom: 1px solid #93969b;
              outline: 0;
              text-align: center;
              line-height: 22px;
              font-size: 16px;
              color: #6f737a;
              width: auto;
            }

            .img-box {
              width: 241px;
              height: 86px;
              margin: 0 auto;
              margin-top: 20px;

              .img {
                width: 100%;
                height: 100%;
              }
            }

            .select-box {
              width: 100%;
              height: auto;

              .selectLi {
                width: 100%;
                height: 40px;
                line-height: 40px;

                label {
                  font-weight: normal;

                  .selectRadio {
                    margin-right: 10px;
                  }
                }
              }
            }

            .answerIcon {
              width: 30px;
              height: 20px;
              font-size: 20px;
              cursor: pointer;
              color: #f959a3;
              display: block;
              margin: 10px 0 0 0;
            }

            .answerBox {
              width: 100%;
              line-height: 30px;
              color: #f959a3;
              display: none;
            }
          }
          .active{
            display: block;
          }
        }
        .item-btn-ul {
          width: 100%;
  
          .item-btn {
            width: 228px;
            height: 45px;
            font-size: 18px;
            text-align: center;
            line-height: 45px;
            border: 1px solid #718c7e;
            border-radius: 5px;
            margin: 8px auto;
            letter-spacing: 2px;
            cursor: pointer;
            color: #04331a;
            transition: all .5s ease;
          }
  
          .answer {
            display: none;
          }
  
          .item-btn.active,
          .item-btn:hover {
            background-color: #f959a3;
            color: #fff;
            border: 1px solid #fba1cb;
          }
        }
