* {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

.main {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.main .left-main {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  width: 575px;
  display: inline-block;
  vertical-align: top;
  overflow: hidden;
}

.main .left-main .bjry-top {
  width: 100%;
  height: 223px;
  background: #fff;
  margin-bottom: 2px;
  padding: 36px 90px;
}

.main .left-main .bjry-top .bjry-title {
  width: 100%;
  height: 60px;
  font-size: 34px;
  line-height: 60px;
  color: #6F737A;
}

.main .left-main .bjry-top .bjry-num {
  width: 100%;
  height: 80px;
  font-size: 26px;
  color: #6F737A;
  font-weight: bold;
  line-height: 55px;
}

.main .left-main .bjry-bottom {
  width: 100%;
  height: 545px;
  background: #fff;
  padding-top: 40px;
}

.main .left-main .bjry-bottom .bjry-title {
  font-size: 26px;
  line-height: 50px;
  color: #6F737A;
  margin-left: 50px;
}

.main .left-main .bjry-bottom .bjry-title span {
  display: inline-block;
  width: 30px;
  height: 30px;
  border-radius: 50%;
  background: #F959A3;
  vertical-align: top;
  margin: 10px 15px 0 5px;
}

.main .left-main .bjry-bottom .bjry-title .greenCircle {
  background: #b3e0ad;
}

.main .left-main .bjry-bottom .bjry-chart {
  width: 100%;
  height: 410px;
}

.main .right-main {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 585px;
  right: 0;
  margin-right: 10px;
  display: inline-block;
  vertical-align: top;
  overflow: hidden;
}

.main .right-main .ksqk-top {
  width: 100%;
  height: 223px;
  padding: 36px 205px 30px 85px;
  color: #6F737A;
  background: #fff;
  margin-bottom: 2px;
  overflow: hidden;
}

.main .right-main .ksqk-top .ksqk-title {
  font-size: 36px;
  line-height: 60px;
}

.main .right-main .ksqk-top .ksqk-num {
  width: 100%;
  height: 70px;
  font-size: 0;
  position: relative;
  font-weight: bold;
  line-height: 45px;
}

.main .right-main .ksqk-top .ksqk-num p {
  display: inline-block;
  width: 50%;
  font-size: 26px;
}

.main .right-main .ksqk-top .ksqk-num .wcqk-bar {
  width: 200px;
  height: 30px;
  position: absolute;
  bottom: -24px;
  right: -135px;
  background: #B3E0AD;
}

.main .right-main .ksqk-top .ksqk-num .wcqk-bar .red-block {
  width: 20%;
  height: 30px;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  background: #F959A3;
}

.main .right-main .ksqk-bottom {
  width: 100%;
  height: calc(100% - 223px);
  background: #fff;
  padding: 40px 50px;
}

.main .right-main .ksqk-bottom .ksqkfx-title {
  font-size: 26px;
  line-height: 50px;
  color: #6F737A;
}

.main .right-main .ksqk-bottom .ksqkfx-title span {
  display: inline-block;
  width: 30px;
  height: 30px;
  border-radius: 50%;
  background: #F959A3;
  vertical-align: top;
  margin: 10px 15px 0 5px;
}

.main .right-main .ksqk-bottom .ksqkfx-title .greenCircle {
  background: #1DFED2;
}

.main .right-main .ksqk-bottom .ksqkfx-title .blueCircle {
  background: #5492FF;
}

.main .right-main .ksqk-bottom .ksqkfx-title .yellowCircle {
  background: #F9E938;
}

.main .right-main .ksqk-bottom .bdscj-chart {
  width: 100%;
  height: 100%;
  overflow: auto;
}

@media screen and (max-width: 1400px) {
  .main .left-main {
    width: 49%;
  }
  .main .left-main .bjry-top {
    height: 210px;
    padding: 25px 30px;
  }
  .main .left-main .bjry-top .bjry-title {
    height: auto;
    font-size: 30px;
    line-height: 50px;
  }
  .main .left-main .bjry-top .bjry-num {
    height: 80px;
    font-size: 18px;
    line-height: 47px;
  }
  .main .left-main .bjry-bottom {
    padding-top: 10px;
  }
  .main .left-main .bjry-bottom .bjry-title {
    font-size: 20px;
    line-height: 30px;
    margin-left: 30px;
  }
  .main .left-main .bjry-bottom .bjry-title span {
    width: 20px;
    height: 20px;
    margin: 6px 15px 0 5px;
  }
  .main .left-main .bjry-bottom .bjry-chart {
    height: 200px;
  }
  .main .right-main {
    left: 51%;
    margin-right: 0px;
  }
  .main .right-main .ksqk-top {
    height: 210px;
    padding: 25px 29px 30px 30px;
  }
  .main .right-main .ksqk-top .ksqk-title {
    font-size: 30px;
    line-height: 50px;
    height: auto;
  }
  .main .right-main .ksqk-top .ksqk-num {
    height: 80px;
  }
  .main .right-main .ksqk-top .ksqk-num p {
    width: 39%;
    font-size: 18px;
    line-height: 45px;
  }
  .main .right-main .ksqk-top .ksqk-num .wcqk-bar {
    width: 120px;
    height: 30px;
    bottom: -7px;
    right: -11px;
  }
  .main .right-main .ksqk-bottom {
    height: calc(100% - 210px);
    padding: 10px 30px;
  }
  .main .right-main .ksqk-bottom .ksqkfx-title {
    font-size: 20px;
    line-height: 30px;
    height: auto;
  }
  .main .right-main .ksqk-bottom .ksqkfx-title span {
    width: 20px;
    height: 20px;
    margin: 7px 15px 0 5px;
  }
}
