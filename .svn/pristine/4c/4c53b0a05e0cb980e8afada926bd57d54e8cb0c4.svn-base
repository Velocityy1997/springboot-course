.main-container {
  display: flex;
  height: 100vh;
  flex-direction: column;
  background-color: #f2f2f2;
}

.main-container .slider-box {
  width: 35px;
  height: 35px;
  background: #fff url(../img/test/slider.png) 0 0 no-repeat;
  background-size: 26px 26px;
  background-position: center;
  position: absolute;
  top: 30px;
  right: 40px;
  cursor: pointer;
  transition: transform .5s ease;
  z-index: 999;
  border-radius: 5px;
}

.main-container .slider-box.active {
  transform: rotate(180deg);
  top: 15px;
}

.main-container .infomation-box {
  flex: 0 0 190px;
  display: flex;
  align-items: center;
  padding-left: 66px;
  margin-bottom: 5px;
  background: #fff;
  position: relative;
  overflow: hidden;
  transition: flex .5s ease;
}

.main-container .infomation-box .admin-img {
  width: 140px;
  height: 160px;
}

.main-container .infomation-box .admin-ul {
  display: flex;
  flex-flow: row wrap;
  font-size: 19px;
  color: #04331a;
  padding-left: 60px;
}

.main-container .infomation-box .admin-ul .admin-li {
  flex: 0 0 30%;
  line-height: 45px;
}

.main-container .infomation-box .admin-ul .admin-li:nth-child(1), .main-container .infomation-box .admin-ul .admin-li:nth-child(4), .main-container .infomation-box .admin-ul .admin-li:nth-child(7) {
  flex: 0 0 30%;
}

.main-container .infomation-box .admin-ul .admin-li:nth-child(2), .main-container .infomation-box .admin-ul .admin-li:nth-child(5), .main-container .infomation-box .admin-ul .admin-li:nth-child(8) {
  flex: 0 0 40%;
}

.main-container .infomation-box.active {
  flex: 0 0 0;
}

.main-container .body {
  flex: auto;
  display: flex;
  flex-direction: column;
  display: none;
}

.main-container .body .tab-control {
  flex: 0 0 120px;
  display: flex;
  background: #fff;
  margin-bottom: 5px;
  position: relative;
}

.main-container .body .tab-control .tab-item {
  flex: 1;
  text-align: center;
  font-size: 24px;
  line-height: 120px;
  color: #04331a;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  position: relative;
}

.main-container .body .tab-control .tab-item .is-test {
  font-size: 16px;
  position: absolute;
  top: 24%;
  right: 28%;
  background-color: #f959a3;
  border-radius: 50%;
  width: 15px;
  height: 15px;
  text-align: center;
  line-height: 20px;
  color: #fff;
  display: none;
}

.main-container .body .tab-control .tab-item .tab-item-img {
  width: 71px;
  height: 81px;
  display: inline-flex;
  transform: scale(0.9);
}

.main-container .body .tab-control .tab-item .tab-item-text {
  display: inline-flex;
  padding-left: 30px;
  font-weight: 300;
  letter-spacing: 2px;
}

.main-container .body .tab-control .tab-item:nth-child(1) .tab-item-img {
  background: url(../img/test/exam.png) 0 0 no-repeat;
  background-size: 100% 100%;
}

.main-container .body .tab-control .tab-item:nth-child(2) .tab-item-img {
  background: url(../img/test/test.png) 0 0 no-repeat;
  background-size: 100% 100%;
}

.main-container .body .tab-control .tab-item:nth-child(3) .tab-item-img {
  background: url(../img/test/myTest.png) 0 0 no-repeat;
  background-size: 100% 100%;
}

.main-container .body .tab-control .tab-item:hover, .main-container .body .tab-control .tab-item.active {
  background-image: linear-gradient(to right, #026938, #b6e2ae);
}

.main-container .body .tab-control .tab-item:hover .tab-item-text, .main-container .body .tab-control .tab-item.active .tab-item-text {
  color: #fff;
}

.main-container .body .tab-control .tab-item:nth-child(1):hover .tab-item-img, .main-container .body .tab-control .tab-item:nth-child(1).active .tab-item-img {
  background: url(../img/test/exam-active.png) 0 0 no-repeat;
  background-size: 100% 100%;
}

.main-container .body .tab-control .tab-item:nth-child(2):hover .tab-item-img, .main-container .body .tab-control .tab-item:nth-child(2).active .tab-item-img {
  background: url(../img/test/test-active.png) 0 0 no-repeat;
  background-size: 100% 100%;
}

.main-container .body .tab-control .tab-item:nth-child(3):hover .tab-item-img, .main-container .body .tab-control .tab-item:nth-child(3).active .tab-item-img {
  background: url(../img/test/myTest-active.png) 0 0 no-repeat;
  background-size: 100% 100%;
}

.main-container .body .body-contain {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.main-container .body .body-iframe {
  border: none;
  width: 100%;
  height: 100%;
  display: none;
}

.main-container .body .machine-iframe {
  border: none;
  width: 100%;
  height: 100%;
  display: none !important;
}

.main-container .body .machine-iframe.active {
  display: block !important;
}

.main-container .body .body-iframe.active {
  display: block;
}

.main-container .body.active {
  display: flex;
}
