<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <style>
        *{
            padding: 0;
            margin: 0;
            box-sizing: border-box;
        }
        .main{
            width: 193px;
            height: 219px;
            overflow: hidden;
            position: relative; 
            color: #fff; 
            
        }
        .tittle{
            width:100%;
            height: 30px;
            line-height: 30px;
            text-align: center;
            font-size: 18px;  
        }
        .searchBox{
            width: 100%;
            padding: 0 5px;
        }
        .searchTypeText{
            width: 100%;
            line-height: 20px;
            font-size: 14px;
        }
        .btnBox {
            width: 170px;
            height: 25px;
            background: #2A2EA2;
            display: inline-block;
            margin-left: 7px;
            text-align: center;
            line-height: 25px;
            color: #fff;
            font-size: 14px;
        }

        .btnBox .leftBtn {
            width: 25px;
            height: 25px;
            border: 1px solid #4D57CB;
            background: url(../../img/machine/message/leftBtn.png) no-repeat;
            background-size: 100% 100%;
            background-position: center;
            float: left;
            cursor: pointer;
        }

        .btnBox .rightBtn {
            width: 25px;
            height: 25px;
            border: 1px solid #4D57CB;
            background: url(../../img/machine/message/rightBtn.png) no-repeat;
            background-size: 100% 100%;
            background-position: center;
            float: right;
            cursor: pointer;
        }
        .idBox{
            width: 100%;
            line-height: 35px;
        }
        .idText{
            font-size: 13px;
        }
        .idInput{
            width: 80px;
            height: 20px;
            border: 1px solid #fff;
            background: #89A7F5;
            box-shadow: inset 0px 15px 1px -15px #000, inset 17px 17px 1px -16px #000;
            padding: 3px;
            display: inline-block;
        }
        .mesPageBtn {
            width: 45px;
            height: 20px;
            border: none;
            border-radius: 3px;
            background: #489CF5;
            color: #fff;
            font-size: 14px;
            line-height: 20px;
            text-align: center;
            cursor: pointer;
        }
        .content{
            width: 100%;
            height: 85px;
            padding: 0 5px;
        }
        .contentUl{
            width: 100%;
            height: 100%;
            border: 1px solid #fff;
            background: #515DE8;
        }
        .mainBottom{
            box-shadow: inset 0px 12px 10px -15px #fff;
            height: 20px;
            padding: 0 5px;
            position: absolute;
            bottom: 0;
            left: 0;
            right: 0;
        }
        .mainReturn{
            width:auto;
            height: 100%;
            float:right;
            cursor: pointer;
        }
        .mainReturn .mainReturn-img{
            width: 15px;
            height: 15px;
            background: url(../../img/machine/return.png) no-repeat;
            background-size: 100% 100%;
            display: inline-block;
        }
        .mainReturn .mainReturn-text{
            font-size: 16px;
            color: #fff;
            display: inline-block;
            line-height: 20px;
            cursor: pointer;
        }
        .mainSure{
            width:auto;
            height: 100%;
            float:left;
            cursor: pointer;
        }
        .mainSure .mainSure-img{
            width: 15px;
            height: 15px;
            background: url(../../img/machine/sure.png) no-repeat;
            background-size: 100% 100%;
            display: inline-block;
        }
        .mainSure .mainSure-text{
            font-size: 16px;
            color: #fff;
            display: inline-block;
            line-height: 20px;
            cursor: pointer;
        }
    </style>
</head>
<body>
    <div class="main">
        <div class="tittle">通信查询</div>
        <div class="searchBox">
            <div class="searchTypeText">查询方式</div>
            <div class="btnBox">
                <button class="leftBtn" id="leftBtn"></button>
                <span class="mesType" id="mesType">按最新存入电文</span>
                <button class="rightBtn" id="rightBtn"></button>
            </div>
            <div class="idBox">
                <span class="idText">查询ID:</span>
                <input type="text" name="" id="" class="idInput">
                <button type="button" class="mesPageBtn">通信薄</button>
            </div>
        </div>
        <div class="content">
            <ul class="contentUl"></ul>
        </div>      
        <div class="mainBottom" >
            <div class="mainSure" id="contentNav">
                    <div class="mainSure-text">查询</div>
                    <div class="mainSure-img nav-img"></div>
            </div>
            <div class="mainReturn" id="mainReturn">
                <div class="mainReturn-img"></div>
                <div class="mainReturn-text">返回</div>
            </div>
        </div>
    </div>
    <script src="../../js/lib/jquery-3.4.1.min.js"></script>
    <script>
        $(function(){
             //返回按钮
        var obj = function(option){
            this.option = option || {};
            this.init();      // 初始化
            }

        // 初始化
        obj.prototype.init = function(){
            this.allEvents(); // 各种点击事件
        }

        // 各种点击事件
        obj.prototype.allEvents = function(){
            this.clickReturn();       // 返回
        }

        // 返回
        obj.prototype.clickReturn = function(){
            $('#mainReturn').on('click', function(){
                window.parent.window.iframeObj.goBackBefore('通讯录');
            });
        }
        new obj();

        })
    </script>
</body>

</html>